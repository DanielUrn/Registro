<div class="container p-4">
    <div class="row">
        <div class="col-md-4 mx-auto">
            <div class="card card-body text-center">
               <a href="/inicio/sede/ingresars"><button class="btn btn-success btn-block">Añadir sede</button></a> 
            </div>
        </div>
    </div>
</div>
<div class="container p-4">

<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nombre</th>
      <th scope="col">Direccion</th>
      <th scope="col">N° Empleados</th>
      <th scope="col">Modificar</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    {{#each lista}}
    <tr>
      <th scope="row">{{idsucursal}}</th>
      <td>{{snombre}}</td>
      <td>{{direccion}}</td>
      <td>{{nempleados}}</td>
      <td><a href="/sede/edits/{{idsucursal}}" class="btn btn-secondary">Editar</a></td>
      
    </tr>
    {{/each}}
  </tbody>
</table>

</div>